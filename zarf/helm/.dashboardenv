# Fill out this file, and renamed to '.dashboard.env' in order to run this with Tilt

NODE_ENV=development
DEV_SERVER_PORT=8081
ENABLE_PROXY=true
API_SERVER=http://localhost:8080

# TRUST_ARN is used when setting the CloudFormation URL for provisioning a cluster
TRUST_ARN=arn:aws:iam::MY_AWS_DEV_ACCOUNT:role/OrganizationAccountAccessRole